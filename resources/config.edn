{
  :web/server {
    :port #or [#long #env CLJWEBAPI_PORT 8080]
    :join? false
    :router #ig/ref :web/router
  }
  :web/router {
    :db #ig/ref :db/mysql
  }
  :db/mysql #merge [{
    :jdbcUrl "jdbc:mysql://localhost:3306/tododb?serverTimezone=UTC&allowPublicKeyRetrieval=true&useSSL=false"
  } #include "secrets-db.edn"]
  #_:auth/jwt #_ #merge [{
    :expiration 900000
  } #include "secrets-jwt.edn"]
}
